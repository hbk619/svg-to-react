  "main": "index.js",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.5.1",
    "eslint-plugin-react": "^7.16.0",
    "mocha": "^6.2.2",
    "sinon": "^7.5.0"
  },
  "bin": {
    "svg-to-react": "./bin/svg-to-react.js"
  },
  "scripts": {
